{
  "manifest_version": 2,

  "name": "HTP Searcher",
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png"}, "description": "This extension searches data at www.park.by",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background":{
    "scripts": ["popup.js"]
  },
  "permissions": [
    "activeTab",
    "http://*.google.com/",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}